@model RentACar.Models.Xidmet
@{
    ViewBag.Title = "CreateXidmet";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Xidmet Yarat</h2>

<form method="post" action="@Url.Action("Create","Xidmet")" enctype="multipart/form-data" role="form">
    <div class="card-body">
        <div class="form-group">
            <label for="exampleInputEmail1">Basliq</label>
            <input type="text" name="Basliq" class="form-control" id="exampleInputEmail1" placeholder="Ad Basliq Edin">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Metn</label>
            <input type="text" name="Metn" class="form-control" id="exampleInputEmail1" placeholder=" Metn Elave Edin">
        </div>



        

    </div>
    <!-- /.card-body -->

    <div class="card-footer">
        <button type="submit" class="btn btn-primary">Yarat</button>
    </div>
</form>