                                </div>
                            </div>
                            <div class="bold-text">@Model.Service.Baslig</div>
                            <div class="text"> @Model.Service.Metn</div>
                            <div class="image">
                                <img src="~/Images/@Model.Service.Sekil" alt="" />
                            </div>
                        </div>
                    </div>
                    <!-- Accordian Column -->
                    <div class="accordian-column col-lg-6 col-md-12 col-sm-12">
                        <div class="inner-column">
                            <!--Accordian Box-->
                            <ul class="accordion-box">
                                <!--Block-->
                                @foreach (var x in Model.RightSides)
                                {
                                    <li class="accordion block">
                                        <div class="acc-btn">
                                            <div class="icon-outer"><span class="acc-icon font-plus fa fa-plus"></span> <span class="acc-icon font-minus fa fa-minus"></span></div>@x.Baslig
                                        </div>
                                        <div class="acc-content">
                                            <div class="content">
                                                <div class="text">@x.Metn</div>
                                            </div>
                                        </div>
                                    </li>
                                }

                                <!--Block-->

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>
