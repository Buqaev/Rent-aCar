<body>
    <div class="page-wrapper">
        <!-- Preloader -->
        <div class="preloader"></div>

        <!-- End Main Header -->
        <!-- Page Title -->
        <section class="page-title" style="background-image: url(/moveo-html/images/background/9.jpg);">
            <div class="auto-container">
                <div class="inner-box">
                    <h1>Bizim xidmətlər</h1>
                    <div class="bread-crumb"><a href="@Url.Action("Index","Page")">AnaSeife &nbsp; ||</a> <i class="current">Xidmet</i></div>
                </div>
            </div>
        </section>
        <!-- End Page Title -->
        <!-- Services Section -->
        <section class="services-section">
            <div class="auto-container">
                <!-- Sec Title -->
                <div class="sec-title centered">
                    <div class="title-inner">
                        <div class="rating">
                            <span class="star fa fa-star"></span>
                            <span class="star fa fa-star"></span>
                            <span class="star fa fa-star"></span>
                        </div>
                        <h2>Biz nə edirik</h2>
                        <div class="text">Sizin konfortunuz ve rahatciliginiz ucun daima xidmetinizdeyik</div>
                    </div>
                </div>
                <div class="row clearfix">
                    <!-- Service Block Three -->
                    @foreach (var x in Model.Xidmets)
                    {
                        <div class="service-block-three col-lg-4 col-md-6 col-sm-12">
                            <div class="inner-box card-box wow fadeInLeft" data-wow-delay="0ms" data-wow-duration="1500ms">
                                <div class="icon-box">
                                    <div class="icon">
                                        <div class="svg-icon" id="svg-1" data-svg-icon="images/svgs/rental.svg">
                                        </div>
                                    </div>
                                </div>
                                <h3><a href="#">@x.Basliq</a></h3>
                                <div class="text">@x.Metn</div>
                                <a href="#" class="read-more">Read More <span class="icon flaticon-right-arrow-forward"></span></a>
                            </div>
                        </div>
                    }

                    <!-- Service Block Three -->

                </div>
            </div>
        </section>
        <!-- End Services Section -->
        <!-- Call To Action Section -->
        <section class="call-to-action-section" style="background-image: url(/moveo-html/images/background/2.jpg)">
            <div class="auto-container">
                <div class="title-box">
                    <h2>
                        Dostlarla səyahətlərinizi yaddaqalan edirik
                        ailə və iş yoldaşlarına daha yaxşı xidmət etmək
                    </h2>
                    <div class="rating">
                        <span class="star fa fa-star"></span>
                        <span class="star fa fa-star"></span>
                        <span class="star fa fa-star"></span>
                    </div>
                    <div class="text">Xüsusilə Toy Nəqliyyatı, Əyləncə Avtobusu Bakalavr və Bachelorette Partiləri və sair kimi hallar üçün..</div>
                </div>
                <div class="cars-image wow zoomIn" data-wow-delay="0ms" data-wow-duration="1500ms">
                    <img src="/moveo-html/images/resource/cars.png" alt="" />
                </div>
            </div>
        </section>
        <!-- End Call To Action Section -->
        <!-- Accordian Section -->
        <section class="accordian-section">
            <div class="auto-container">
                <div class="row clearfix">
