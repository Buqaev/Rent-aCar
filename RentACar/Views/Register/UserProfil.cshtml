@model RentACar.Models.UserDto
@{
    ViewBag.Title = "UserProfil";
    Layout = "~/Views/Shared/_PanelLayout.cshtml";
}

<!DOCTYPE html>
<html lang="en">


<!-- Mirrored from www.shmai.com/preview/moveo-html/dashboard.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 11 Jul 2022 18:00:07 GMT -->
<head>

    <link href="~/moveo-html/css/style.css" rel="stylesheet">
   
</head>

<body>

    <div class="page-wrapper">
        <!-- Preloader -->
        <div class="preloader"></div>
        
        <!-- End Main Header -->
        <!-- Page Title -->
        <section class="page-title" style="background-image:url(images/background/9.jpg);">
            <div class="auto-container">
                <div class="inner-box">
                    <h1>Dashboard</h1>
                    <div class="bread-crumb">
                        <a href="index-2.html">Home &nbsp; ||</a> <i class="current">Dashboard</i>
                    </div>
                </div>
            </div>
        </section>

        <section class="container" style="margin-top:20px;">
            <table class="table table-active">
                <thead>
                    <tr>
                        <td>Ad</td>
                        <td>Soyad</td>
                        <td>Mail</td>
                        <td>Sifre</td>
                        <td>Sekil</td>
                        <td>Yenileme</td>
                    </tr>
                </thead>
                <tr>
                    <td>@Model.User.Ad</td>
                    <td>@Model.User.Soyad</td>
                    <td>@Model.User.Mail</td>
                    <td>@Model.User.Sifre</td>
                    <td>
                        <img height="40px" width="50px" src="~/Images/@Model.User.Sekil" />
                    </td>
                    <td>
                        <a class="btn btn-primary" href="@Url.Action("UserUpdate","Register",new {Id=Model.User.Id })">Yenile</a>
                    </td>
                </tr>
            </table>
        </section>

        <!-- End Page Title -->
        <!-- Dashboard Container-->
        <div class="dashboard-container">
            <div class="auto-container">
                <!-- Info Box -->
                <div class="info-box">
                    <div class="info-inner">
                        <div class="image">
                            <img src="~/Images/@Model.User.Sekil" alt="" />
                        </div>
                        <h4>@Model.User.Ad</h4>
                        <a href="#" class="logout">Çıxış</a>
                    </div>
                </div>
                <!-- Welcome -->
                <div class="welcome">
                    <span class="fa fa-exclamation-circle"></span> Moveo Vendor Dashboard-a xoş gəlmisiniz.
                </div>
                <!-- Dashboard Option -->
                <div class="dashboard-options">
                    <div class="row clearfix">
                        <!-- Dashboard Box -->
                        <div class="dashboard-box col-lg-4 col-md-6 col-sm-12">
                            <div class="dashboard-inner card-box">
                                <div class="icon">
                                    <div class="svg-icon" id="svg-1" data-svg-icon="/moveo-html/images/svgs/moveo-20.svg">
                                    </div>
                                </div>
                                <h4>@ViewBag.MasinSay</h4>
                                <div class="text">Alinan Ümumi Nəqliyyat vasitələri</div>
                            </div>
                        </div>
                        <!-- Dashboard Box -->
                        <div class="dashboard-box col-lg-4 col-md-6 col-sm-12">
                            <div class="dashboard-inner card-box">
                                <div class="icon">
                                    <div class="svg-icon" id="svg-2" data-svg-icon="images/svgs/moveo-16.svg">
                                    </div>
                                </div>
                                <h4>1145</h4>
                                <div class="text">Total No. of Trips</div>
                            </div>
                        </div>
                        <!-- Dashboard Box -->
                        <div class="dashboard-box col-lg-4 col-md-6 col-sm-12">
                            <div class="dashboard-inner card-box">
                                <div class="icon">
                                    <div class="svg-icon" id="svg-3" data-svg-icon="images/svgs/moveo-17.svg">
                                    </div>
                                </div>
                                <h4>$1,560</h4>
                                <div class="text">Satış</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Dashboard Btns -->
            <div class="dashboard-btns-box">
                <div class="auto-container">
                    <a href="#" class="theme-btn btn-style-two"><span class="txt">Reserve Now</span></a>
                    <a href="#" class="theme-btn btn-style-four"><span class="txt">Get a Quote</span></a>
                </div>
            </div>
            <div class="auto-container">
                <!-- Dashboard Table -->
                <div class="dashboard-table">
                    <div class="dashboard-inner">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>MUsderi <span class="caret fa fa-caret-down"></span></th>
                                    <th>Masin <span class="caret fa fa-caret-down"></span></th>
                                    <th>Model <span class="caret fa fa-caret-down"></span></th>
                                    
                                    <th>Qiymet <span class="caret fa fa-caret-down"></span></th>
                                    <th>Alinma Tarixi <span class="caret fa fa-caret-down"></span></th>
                                    <th>Qaytarilma Tarixi <span class="caret fa fa-caret-down"></span></th>
                                    <th>Odenilecek Mebleg <span class="caret fa fa-caret-down"></span></th>
                                    <th>Sekil <span class="caret fa fa-caret-down"></span></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var x in Model.Rezervs)
                                {
                                    <tr>
                                        <td>@x.User.Ad</td>
                                        <td>@x.Car.CarMarka.Ad</td>
                                        <td>@x.Car.Model</td>
                                        <td>$ @x.Car.Qiymet1_3</td>
                                        <td>@x.AlinmaTarix</td>
                                        <td>@x.QaytarilmaTarix</td>
                                        
                                        
                                        <td>$@x.UmmiMebleg</td>
                                        <td><span class="dots"><img height="70px;" width="60px;" src="~/Images/@x.Car.Sekil" alt="" /></span></td>
                                    </tr>
                                }


                            </tbody>
                        </table>
                        <!-- Styled Pagination -->
                        
                    </div>
                </div>
            </div>
        </div>
        <!-- End Dashboard Container-->
        <!-- Main Footer -->
        
        <!-- End Main Footer -->
    </div>

    

</body>


<!-- Mirrored from www.shmai.com/preview/moveo-html/dashboard.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 11 Jul 2022 18:00:12 GMT -->
</html>

