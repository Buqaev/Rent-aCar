                                                                    <h2>Şərhlər</h2>
                                                                    <div class="rating">
                                                                        <i class="star fa fa-star"></i>
                                                                    </div>
                                                                </div>
                                                                <!-- Comment text -->
                                                                @foreach (var x in Model.CarsReviews)
                                                                {
                                                                    <div class="content">
                                                                        <div class="image wow zoomIn" data-wow-delay="0ms" data-wow-duration="1500ms">
                                                                            <img style="height:70px;border-radius:100%" src="~/Images/@x.User.Sekil" alt="image">
                                                                        </div>
                                                                        <div class="text">
                                                                            <ul>
                                                                                <li>@x.User.Ad @x.User.Soyad</li>
