                            <span class="star fa fa-star"></span>
                            <span class="star fa fa-star"></span>
                        </div>
                        <h2>Related Vehicles</h2>
                    </div>
                </div>
                <div class="row clearfix">
                    @foreach (var x in Model.Cars)
                    {
                        <div class="project-block-four col-lg-3 col-md-6 col-sm-12">
                            <div class="inner-box">
                                <div class="image">
                                    <a href="fleet-detail.html"><img src="~/Images/@x.Sekil" alt="" /></a>
                                </div>
                                <div class="lower-content">
                                    <h3><a href="fleet-detail.html">@x.CarMarka.Ad</a></h3>

                                    <div class="booking">
                                        <a href="@Url.Action("CarPageDetail","CarPage",new { Id=x.Id})" class="theme-btn book">Book now</a>
                                        <div class="heart flaticon-heart"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }

                    <!-- Project Block Four -->

                </div>
