                                            <div class="brand"><img src="~/moveo-html/images/icons/brand-1.png" alt="" /></div>
                                        </div>
                                    </div>
                                </div>









                                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                        @{
                                            var first = true;
                                        }
                                        @foreach (var x in Model.CarKarusels)
                                        {

                                            <div class="carousel-item @(first?Html.Raw("active"):Html.Raw(""))">
                                                <img class="d-block w-100" src="~/Images/@x.Car.Sekil" alt="@x.Car.Model">
                                            </div>
                                            first = false;
                                        }
                                    </div>
                                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button"
                                       data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carouselExampleControls" role="button"
                                       data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </div>













