                                    <!-- Fleet Form Two -->
                                    <div class="fleet-form-two">
                                        <form method="post" action="http://www.shmai.com/preview/moveo-html/contact.html">
                                            <div class="widget-inner">
                                                <div class="row clearfix">
                                                    <!-- Form Group -->
                                                    <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                                        <label>Götürmə yeri</label>
                                                        <input type="text" name="location" value="" placeholder="Enter Location" required>
                                                        <span class="icon fas fa-map-marker-alt"></span>
                                                    </div>
                                                    <!-- Form Group -->
                                                    <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                                        <label>Təyinat</label>
                                                        <input type="text" name="location" value="" placeholder="Enter Location" required>
                                                        <span class="icon fas fa-map-marker-alt"></span>
                                                    </div>
                                                    <!-- Form Group -->
                                                    <div class="form-group margin col-lg-12 col-md-12 col-sm-12">
                                                        <label>Pickup Date</label>
                                                        <input class="datepicker" type="text" name="location" value="" placeholder="Select Date" required>
                                                        <span class="icon far fa-calendar-alt"></span>
                                                    </div>
                                                    <!-- Form Group -->
                                                    <div class="form-group col-lg-6 col-md-6 col-sm-12">
                                                        <input type="text" placeholder="Hours" required>
                                                    </div>
                                                    <!-- Form Group -->
                                                    <div class="form-group col-lg-6 col-md-6 col-sm-12">
                                                        <input type="text" placeholder="Mins" required>
                                                    </div>
                                                    <!-- Form Group -->
                                                    <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                                        <div class="select-box">
                                                            <input type="checkbox" name="payment-group" id="radio-one">
                                                            <label for="radio-one">
                                                                <span class="default-check"></span>
                                                                <span class="check-icon fa fa-check"></span>
                                                                I agreed to <a href="#">Terns & Conditions</a>
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <!-- Form Group -->
